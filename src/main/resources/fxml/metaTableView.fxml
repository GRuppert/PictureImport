<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<TableView editable="true" items="${controller.meta}" xmlns:fx="http://javafx.com/fxml/1" prefHeight="800" prefWidth="1200" fx:controller="org.nyusziful.pictureorganizer.UI.Contoller.MetaTableViewController">
    <columnResizePolicy><TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/></columnResizePolicy>
    <columns>
        <TableColumn fx:id="processingCol" text="dateFormat" prefWidth="50" resizable="false" editable="true">
            <cellValueFactory><PropertyValueFactory property="dateFormat" />
            </cellValueFactory>
        </TableColumn>
        <TableColumn fx:id="nameCol" text="Left Filename">
            <cellValueFactory><PropertyValueFactory property="originalFilename" />
            </cellValueFactory>
        </TableColumn>
        <TableColumn fx:id="dateCol" text="Date" >
            <cellValueFactory><PropertyValueFactory property="date" />
            </cellValueFactory>
        </TableColumn>
        <TableColumn fx:id="modelCol" text="Model">
            <cellValueFactory><PropertyValueFactory property="model" />
            </cellValueFactory>
        </TableColumn>
    </columns>
</TableView>
