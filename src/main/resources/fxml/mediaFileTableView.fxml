<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<TableView editable="true" items="${controller.dataModel}" xmlns:fx="http://javafx.com/fxml/1" id="BorderPane" prefHeight="800" prefWidth="1200" fx:controller="org.nyusziful.Rename.TablePanelController">
    <columnResizePolicy><TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/></columnResizePolicy>
    <columns>
        <TableColumn fx:id="processingCol" text="Ok" prefWidth="50" resizable="false" editable="true">
            <cellValueFactory><PropertyValueFactory property="processing" />
            </cellValueFactory>
        </TableColumn>
        <TableColumn fx:id="oldNameCol" text="Current Filename">
            <cellValueFactory><PropertyValueFactory property="firstName" />
            </cellValueFactory>
        </TableColumn>
        <TableColumn fx:id="newNameCol" text="After Rename">
            <cellValueFactory><PropertyValueFactory property="newName" />
            </cellValueFactory>
        </TableColumn>
        <TableColumn fx:id="noteCol" text="Remarks">
            <cellValueFactory><PropertyValueFactory property="note" />
            </cellValueFactory>
        </TableColumn>
        <TableColumn fx:id="xmpCol" text="xmp" prefWidth="50" resizable="false">
            <cellValueFactory><PropertyValueFactory property="xmpMissing" />
            </cellValueFactory>
        </TableColumn>
    </columns>
</TableView>
