<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.control.cell.CheckBoxTableCell?>
<TableView fx:id="Table" editable="true" xmlns:fx="http://javafx.com/fxml/1" prefHeight="800" prefWidth="1200" fx:controller="org.nyusziful.pictureorganizer.UI.Contoller.Rename.MediaFileTableViewController">
    <columnResizePolicy><TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/></columnResizePolicy>
    <columns>
        <TableColumn fx:id="processingCol" text="Ok" prefWidth="50" resizable="false" editable="true">
            <cellValueFactory><PropertyValueFactory property="processing" />
            </cellValueFactory>
        </TableColumn>
        <TableColumn fx:id="oldNameCol" text="Current Filename">
            <cellValueFactory><PropertyValueFactory property="currentName" />
            </cellValueFactory>
        </TableColumn>
        <TableColumn fx:id="newNameCol" text="After Rename">
            <cellValueFactory><PropertyValueFactory property="newName" />
            </cellValueFactory>
        </TableColumn>
        <TableColumn fx:id="noteCol" text="Remarks">
            <cellValueFactory><PropertyValueFactory property="note" />
            </cellValueFactory>
        </TableColumn>
        <TableColumn fx:id="xmpCol" text="xmp" prefWidth="50" resizable="false">
            <cellValueFactory>
                <PropertyValueFactory property="xmpMissing" />
            </cellValueFactory>
        </TableColumn>
    </columns>
</TableView>
